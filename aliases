 # FORCE SPECIFIC PATHS AND ARGUMENTS
# alias gcc cc
alias lp "lp -w"
alias pushd  'pushd \!*; cd .'
alias popd  'popd \!*; cd .'
alias lpr 'lpr -h'	# skip header page on print jobs
alias setcl 'setenv CHANGELIST `p4 changes -m1 //kbukin_bringup_g80/sw/pvt/...#have | cut -d" " -f2`'


if ($OSTYPE == "SunOS") then
  # these /home/gnu programs are sun-only

  alias ls '/home/gnu/bin/dir -CF'        
  alias mv '/home/gnu/bin/mv -i'	# prompt before overwriting file
  alias cp '/home/gnu/bin/cp -i'	# prompt before overwriting file
  # alias rm '/home/gnu/bin/rm -i'	# prompt before removing file
