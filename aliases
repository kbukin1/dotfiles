alias man 'man -a'


 # PRINTING ALIASES
alias printstdio2  "enscript --fancy-header --no-job-header --landscape --columns=2 --font=Courier7 -DDuplex:true --output=- \!*"
alias printstdio2a "enscript --fancy-header --no-job-header --landscape --columns=2 --font=Courier7 -DDuplex:false --output=- \!*"
alias printstdio1  "enscript --fancy-header --no-job-header --portrait --columns=1 --font=Courier9 -DDuplex:false --output=- \!*"
alias print1  "printstdio1 \!* | lp -d$PRINTER -w"
alias print1w "printstdio1 --landscape --font=Courier7 \!* | lp -d$PRINTER -w"
alias print2  "printstdio2 \!* | lp -d$PRINTER -w"
alias print1f "printstdio1 -p\!*.ps \!*"
alias print2f "printstdio2 -p\!*.ps \!*"
alias print2f "printstdio2 -p\!*.ps \!*"
alias print   "print2"
alias enscriptp "print -p\!*.ps \!*"


 # TERMINAL ALIASES
alias xterm_sized "xterm -sb -sl 600 -fg white -bg gray40 -cr yellow -fn 9x15 -name $HNAME"
alias xx "xterm_sized \!* &"
alias xh "xterm_sized -fg black -bg white &"
alias xs "nohup xterm_sized & ; exit"
alias xn "xterm_sized -title \!* &"
alias wx "winterm -geometry 80x24"
if ($?DISPLAY) then
  alias fastcase "xterm -bg gray90 -fg darkgreen -sb -sl 2000 -display $DISPLAY -title FAST_CASE"
  alias slowcase "xterm -bg gray90 -fg darkred -sb -sl 2000 -display $DISPLAY -title SLOW_CASE"
  alias bfastcase "bsub -q primetime4g fastcase"
  alias bslowcase "bsub -q primetime4g slowcase"
endif


 # ENABLE/DISABLE "TRACE" MESSAGES

alias trace 'perl -pi -e "s/^\#\#echo/echo/;"' ~/.cshrc ~/.cshrc_tools ~/.login ~/.cshrc_custom ~/.aliases
alias untrace 'perl -pi -e "print chr(35).chr(35) if (/^echo .trace/);"' ~/.cshrc ~/.cshrc_tools ~/.login ~/.cshrc_custom ~/.aliases


