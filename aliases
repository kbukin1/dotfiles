 # set emacs to use the file name if we have a display
if ($?DISPLAY) then
  alias e "emacs -bg gray90"
   # setenv FNAME will resolve any environment vars
  alias en 'setenv FNAME \!*; e -fn 8x13 -name `basename $FNAME` \!* &'
  alias es 'setenv FNAME \!*; e -fn 6x12 -name `basename $FNAME` \!* &'
else
  alias e "emacs"
   # setenv FNAME will resolve any environment vars
  alias en e
  alias es e
endif

