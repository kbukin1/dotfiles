# removing . from $PATH
setenv PATH `echo $PATH | sed 's/:\.:/:/g'`
setenv MESA_NO_3DNOW 1

#setup env if this is beings sourced from someone else's shell
if (! $?HAME) setenv HNAME `hostname`
if (! $?SEP)  setenv SEP   ":"

# ABBREVIATIONS
alias a alias
alias c clear
alias g grep
alias h history
alias grep grep --colour=auto

alias dir ls -l
alias ls ls -F
alias ll ls -l
alias l ls
alias hg "h | g \!*"
alias po popd
alias pu pushd

alias gv ghostview
alias od od -tx4 -Ax
alias od2 xxd -g4
# alias cc /home/utils/gcc-4.2.4/bin/g++
alias tkdiff ~/usr/bin/tkdiff

alias s 'pwd > ~/.saved_dir/\!*'
alias p 'pd `cat ~/.saved_dir/\!*`'
alias g 'cd `cat ~/.saved_dir/\!*`'
alias go 'cd `cat ~/.saved_dir/\!*`'
alias so source

alias vi /home/utils/vim-7.4/bin/vim
alias p4 'p4 -d `/bin/pwd`'

alias qsh 'qsub -P kepler -q o_pri_interactive_cpu_2G  -Is tcsh'

alias cd.. "cd .."
alias make_mdiag "make Build MDIAG_PARALLELMAKE=1 NV_DISTCC=1"
alias make_mdiag_rel "make Build MDIAG_PARALLELMAKE=1 NV_DISTCC=1 OPT_LEVEL=1"
alias cp cp -i

# FORCE SPECIFIC PATHS AND ARGUMENTS
alias lp "lp -w"
alias pushd  'pushd \!*; cd .'
alias popd  'popd \!*; cd .'
alias lpr 'lpr -h'	# skip header page on print jobs
alias setcl 'setenv CHANGELIST `p4 changes -m1 //kbukin_bringup_g80/sw/pvt/...#have | cut -d" " -f2`'

if ($OSTYPE == "SunOS") then
  # these /home/gnu programs are sun-only

  alias ls '/home/gnu/bin/dir -CF'        
  alias mv '/home/gnu/bin/mv -i'	# prompt before overwriting file
  alias cp '/home/gnu/bin/cp -i'	# prompt before overwriting file
  # alias rm '/home/gnu/bin/rm -i'	# prompt before removing file
  # alias make gmake
  # alias make smake
  alias elm "/home/alben/bin/elm"
  alias mail Mail
  alias df "/home/gnu/bin/df"
  alias diff "/home/gnu/bin/diff"
endif

alias man 'man -a'

# PRINTING ALIASES
alias printstdio2  "enscript --fancy-header --no-job-header --landscape --columns=2 --font=Courier7 -DDuplex:true --output=- \!*"
alias printstdio2a "enscript --fancy-header --no-job-header --landscape --columns=2 --font=Courier7 -DDuplex:false --output=- \!*"
alias printstdio1  "enscript --fancy-header --no-job-header --portrait --columns=1 --font=Courier9 -DDuplex:false --output=- \!*"
alias print1  "printstdio1 \!* | lp -d$PRINTER -w"
alias print1w "printstdio1 --landscape --font=Courier7 \!* | lp -d$PRINTER -w"
alias print2  "printstdio2 \!* | lp -d$PRINTER -w"
alias print1f "printstdio1 -p\!*.ps \!*"
alias print2f "printstdio2 -p\!*.ps \!*"
alias print2f "printstdio2 -p\!*.ps \!*"
alias print   "print2"
alias enscriptp "print -p\!*.ps \!*"


# ENABLE/DISABLE "TRACE" MESSAGES
alias trace 'perl -pi -e "s/^\#\#echo/echo/;"' ~/.cshrc ~/.cshrc_tools ~/.login ~/.cshrc_custom ~/.aliases
alias untrace 'perl -pi -e "print chr(35).chr(35) if (/^echo .trace/);"' ~/.cshrc ~/.cshrc_tools ~/.login ~/.cshrc_custom ~/.aliases

alias checkbuild "egrep -i '(\*\*|error)' \!* | grep -v ignore"
alias nv 'cd $NV'

alias errgrep 'grep Error \!* | egrep -v "msg =|Test-Compiler"|more'

alias dos2unix "perl -pi -e 's/\cM//g;'"
alias unix2dos "perl -pi -e 's/\n/\cM\n/m;'"

alias codep_checking = " ./bin/autosubmit \
                        -srcClient kbukin_mods   \
                        -destClient kbukin_mods_submit \
                        -revert \
                        -debug  \
                        "
